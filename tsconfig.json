
{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6"],
    "sourceMap": true,
    "outDir": "./dist",

    "skipLibCheck" : true,
    "declaration" : true,
    "noImplicitAny" : true,
    "noImplicitReturns" : true,
    "noImplicitThis" : true,
    "strictNullChecks" : true
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
